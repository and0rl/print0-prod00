generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./list.db"
}

model Processed {
  uid           Int           @id
  date          DateTime
  fromname      String?
  from          String?
  subject       String?
  extractedName String?
  attachments   Attachments[]
  //attachments String
}

model Attachments {
  createdAt   DateTime //@default(now())
  id          Int       @id @default(autoincrement())
  file        String
  processed   Processed @relation(fields: [processedId], references: [uid])
  processedId Int
  //Processed    Processed? @relation(fields: [processedUid], references: [uid])
  //processedUid Int?
  //processed Processed[] @relation("aaa")
}

